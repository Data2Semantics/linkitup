PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT DISTINCT ?s WHERE {
  ?s rdfs:label ?l .
  FILTER(regex(str(?l), "{{ tag }}", "i")) 
  FILTER (!regex(str(?s), '^http://neurolex.org/wiki/Help-')).
  FILTER (!regex(str(?s), '^http://neurolex.org/wiki/File-')).
  FILTER (!regex(str(?s), '^http://neurolex.org/wiki/Special:')).
} LIMIT 5